


<div class="col-md-12 offset-md-0">
    <div class="card">
        <div class="dos card-header ">
            <div class="form-header text-center">
                <h1 class="form-title">P<span>articipantes</span></h1>
                                <a class="btn btn-outline-info " href="/">
                    <h1>
                        <span>Inicio</span>
                        <i class="bi bi-house "></i>
                    </h1>
                </a>
                <a class="btn btn-outline-success " href="/createnames">
                    <h1>
                        <span>Nuevo</span>
                        <i class="bi bi-plus-circle-fill "></i>
                    </h1>
                </a>
                <a class="btn btn-outline-secondary " href="/namesall">
                    <h1>
                        <span>Historial</span>
                        <i class="bi bi-clock-history"></i>
                    </h1>
                </a>
            </div>
        </div>
        <div class="uno card-body">


            <section class="section">
                <div class="row">
                    <div class="col-lg-12">

                        <div class="card">
                            <div class="card-body">


                                <!-- Table with stripped rows -->
                                <table class="table datatable">
                                    <thead>
                                        <tr>
                                            <th class="size1 text-center">
                                                <h1>Nombre</h1>
                                            </th>
                                            <th class="size1 text-center">
                                                <h1>Cédula</h1>
                                            </th>
                                            <th class="text-center">
                                                <h1>Teléfono</h1>
                                            </th>
                                            <th class="size text-center">
                                                <h1>Modelo</h1>
                                            </th>
                                            <th class="size">
                                                <h1></h1>
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {{#each nombres}}
                                        <tr>
                                            <td>
                                                <h2>{{this.nombre}}</h2>
                                            </td>
                                            <td>
                                                <h2>{{this.cedula}}</h2>
                                            </td>
                                            <td>
                                                <h2>{{this.telefono}}</h2>
                                            </td>
                                            <td>
                                                <h2>{{this.marca_dell}}</h2>
                                            </td>
                                            <td class="text-center ">
                                                <abbr  title="enviar nombre"><a href="   /namesen/{{this.id}}"
                                                        class="btn btn-outline-info p-2" id="lightOn">
                                                        <h1><i class="bi bi-arrow-right-circle"></i></h1>
                                                    </a></abbr>
                                                <abbr title="Eliminar"><a 
                                                        href="deletename/{{this.id}}"
                                                        class="panel-transparent btn btn-outline-danger p-2">
                                                        <h1><i class="bi bi-trash"></i></h1>
                                                    </a></abbr>
                                            </td>

                                        </tr>
                                        </tr>
                                        {{/each }}
                                    </tbody>
                                </table>
                                <!-- End Table with stripped rows -->

                            </div>
                        </div>

                    </div>
                </div>
            </section>
        </div>
    </div>
</div>
<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>


